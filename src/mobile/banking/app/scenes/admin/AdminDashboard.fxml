               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font>
            </Button>
            <Button fx:id="transactions" layoutX="408.0" layoutY="288.0" lineSpacing="5.0" mnemonicParsing="false" onAction="#transactions_Handler" text="TRANSACTIONS" underline="true">
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font>
            </Button>
            <Label layoutX="144.0" layoutY="99.0" text="ADMIN DASHBOARD">
               <font>
                  <Font name="Tahoma Bold" size="36.0" />
               </font>
            </Label>
            <Separator layoutX="231.0" layoutY="275.0" orientation="VERTICAL" prefHeight="90.0" prefWidth="11.0" />
            <Separator layoutX="390.0" layoutY="276.0" orientation="VERTICAL" prefHeight="90.0" prefWidth="11.0" />
            <ImageView fitHeight="103.0" fitWidth="200.0" layoutX="231.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../client/icons/Red-Hat-new-logo-770x285.jpg" />
               </image>
            </ImageView>
            <Label layoutX="283.0" layoutY="145.0" text="Ghostx.Dev">
               <font>
                  <Font name="System Bold Italic" size="14.0" />
               </font>
            </Label>
         </children>
    </AnchorPane>
  </children>
</VBox>
